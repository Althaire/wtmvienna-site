---
layout: nil
---

[
{% for entry in site.data.news %}
    {
      "title"    : "{{ entry.title }}",
      "image"    : "{{ entry.img }}",
      "url"      : "{{ entry.url }}",
      "date"     : "{{ entry.date | date: "%B %d, %Y" }}",
      "content"  : "{{ entry.content | escape }}",
      "priority" : {{ entry.priority }}
    } {% if forloop.last %}{% else %},{% endif %}
{% endfor %}
]
