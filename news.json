---
layout: nil
---

{ "news" : [
  {% for entry in site.data.news %}
    {
      "title"    : "{{ entry.title }}",
      "image"    : "{{ entry.image }}",
      "url"      : "{{ entry.url }}",
      "date"     : "{{ entry.date | date: "%Y-%m-%d %H:%M:%S" }}",
      "content"  : "{{ entry.content | escape }}",
      "priority" : {{ entry.priority }}
    } {% if forloop.last %}{% else %},{% endif %}
  {% endfor %}
  ]
}
